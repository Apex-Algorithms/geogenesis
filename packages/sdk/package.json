		"viem": "^1.16.0"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.16.4",
		"@bufbuild/buf": "^1.31.0",
		"@bufbuild/protoc-gen-es": "^1.9.0",
		"@types/uuid": "^9.0.8",
		"tsup": "^8.3.0",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vitest": "^1.6.0"
	}
}
