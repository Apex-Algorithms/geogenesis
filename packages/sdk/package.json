{
	"name": "@geobrowser/gdk",
	"version": "0.0.1",
	"license": "MIT",
	"module": "./dist/index.js",
	"main": "./dist/index.ts",
	"ascMain": "./index.ts",
	"type": "module",
	"exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
		"./constants": {
			"types": "./dist/constants.d.ts",
			"import": "./dist/constants.js"
		},
    "./contracts": {
      "types": "./dist/contracts.d.ts",
      "import": "./dist/contracts.js"
    },
    "./proto": {
      "types": "./dist/proto.d.ts",
      "import": "./dist/proto.js"
    },
		"./abis": {
			"types": "./dist/abis.d.ts",
			"import": "./dist/abis.js"
		},
		"./ids": {
			"types": "./dist/ids.d.ts",
			"import": "./dist/ids.js"
		}
  },
	"files": [
		"dist"
	],
	"homepage": "https://geobrowser.io",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/geobrowser/geogenesis.git"
	},
	"keywords": [
		"geo",
		"the graph",
		"knowledge graph"
	],
	"bugs": {
		"url": "https://github.com/geobrowser/geogenesis/issues"
	},
	"scripts": {
		"build": "tsup",
		"test": "vitest",
		"generate:protobuf": "npx buf generate",
		"check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm"
	},
	"dependencies": {
		"@biomejs/biome": "^1.9.2",
		"@bufbuild/protobuf": "^1.9.0",
