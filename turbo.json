{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["codegen", "^build"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "codegen": {
      "dependsOn": ["^codegen"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "test": {
      "dependsOn": ["build"]
    },
    "web": {
      "outputs": [".next/**"]
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint", "format"],
      "outputs": []
    },
    "@geogenesis/action-schema": {
      "outputs": ["dist/**", "build/**", "assembly/**"]
    },
    "@geogenesis/contracts": {
      "outputs": ["dist/**", "build/**"]
    },
    "@geogenesis/subgraph": {
      "outputs": ["build/**", "generated/**"]
    }
  }
}
